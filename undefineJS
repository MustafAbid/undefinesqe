    <p id="gfg"></p>
  
    <script>
        function checkVar() {
            if (typeof variable === "undefined") {   
                string = "Variable is undefined";
            } else {   
                string = "Variable is defined";
            }
            document.getElementById("gfg").innerHTML = 
              string;
        }
    </script>
  
